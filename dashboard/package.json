{
  "name": "eva-network-dashboard",
  "version": "1.0.0",
  "description": "EVA Network Command Center - SSH-based container management",
  "main": "ssh-control-api.js",
  "scripts": {
    "start": "node ssh-control-api.js",
    "dev": "nodemon ssh-control-api.js",
    "docker:build": "docker build -t eva-dashboard .",
    "docker:run": "docker run -d -p 3333:3333 -v ~/.ssh:/root/.ssh:ro --name eva-dashboard eva-dashboard"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ssh2": "^1.15.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "eva",
    "network",
    "ssh",
    "docker",
    "management",
    "dashboard"
  ],
  "author": "Jordan Ehrig",
  "license": "MIT"
}
